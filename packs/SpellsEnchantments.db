{"name":"Phasing","type":"Enchantment","img":"systems/newera-sol366/resources/physiomancy.png","_id":"CASPEREN00000055","system":{"description":"<p>A creature wearing this item can walk through solid walls and other obstacles not protected by magical barriers.</p>\n</p>The creature must move normally and at its normal speed to pass through the wall while the enchantment is active.</p><p>Rulings: </p><p>The creature may be walking or crawling when passing through the wall, but may not use other movement actions such as Sprint or Long Jump.</p>","rarity":2,"casperObjectId":55,"level":3,"energyCost":65,"school":"MA","form":"physical","keywords":"Complex, Static","static":true,"refinementLevel":"0","enchantedItemDescriptor":null,"materialCosts":{"0":{"name":"Ætherium","quantity":"2","scales":true,"unique":false},"1":{"name":"Ruinstone","quantity":"1","scales":false,"unique":false},"2":{"name":"Ectoplasm","quantity":"1","scales":false,"unique":false}},"validTargets":{"melee":false,"ranged":false,"clothing":true,"accessory":true,"shield":false,"creature":false,"object":false,"other":false,"details":""},"enchantmentType":"CE","components":{"0":{"name":"Activation","level":1,"scales":false,"check":"MM","energyCost":15},"1":{"name":"Selective Interaction","level":5,"scales":false,"check":"MM","energyCost":20}},"specialty":"physiomancy","maxCharges":0,"scaleCharges":false,"actions":{},"ampFactor":1},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.16.1","coreVersion":"12.330","createdTime":1738093358109,"modifiedTime":1738093358109,"lastModifiedBy":null}}
