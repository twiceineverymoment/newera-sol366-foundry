{"name":"Expert Archer","type":"Feat","img":"systems/newera-sol366/resources/ranger.png","_id":"CASPERFE00000461","system":{"description":"","rarity":null,"casperObjectId":461,"displayName":"","featType":"CF","featSubType":"Ranger","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":20,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":20,"description":"<p>You can quick-fire a bow in one frame.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561053,"modifiedTime":1713405561053,"lastModifiedBy":null}}
{"name":"Relentless Pursuit","type":"Feat","img":"systems/newera-sol366/resources/ranger.png","_id":"CASPERFE00000462","system":{"description":"","rarity":null,"casperObjectId":462,"displayName":"","featType":"CF","featSubType":"Ranger","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>Attacks against your Prey are exempt from the Repetition Tax.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561055,"modifiedTime":1713405561055,"lastModifiedBy":null}}
{"name":"Diverse Skill Set","type":"Feat","img":"systems/newera-sol366/resources/ranger.png","_id":"CASPERFE00000464","system":{"description":"","rarity":null,"casperObjectId":464,"displayName":"","featType":"CF","featSubType":"Ranger","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":60,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":60,"description":"<p>Gain an additional natural skill from the Ranger's Natural Skills list. Increase that skill's level by 1.</p>\n</p>You can now use your Primal Sense on this skill.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561056,"modifiedTime":1713405561056,"lastModifiedBy":null}}
{"name":"Jack of All Trades","type":"Feat","img":"systems/newera-sol366/resources/ranger.png","_id":"CASPERFE00000465","system":{"description":"","rarity":null,"casperObjectId":465,"displayName":"","featType":"CF","featSubType":"Ranger","currentTier":1,"maximumTier":-1,"isUpgrade":true,"isSingleTiered":false,"totalCost":40,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":40,"description":"<p>Gain one level in one of the specialties listed under the Ranger Specialties feature.</p>\n</p>If you gain a new specialty this way, you can now use your Primal Sense on that action.|(Specialties can never be increased above 3.)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561057,"modifiedTime":1713405561057,"lastModifiedBy":null}}
{"name":"Hunter's Aim","type":"Feat","img":"systems/newera-sol366/resources/ranger.png","_id":"CASPERFE00000466","system":{"description":"","rarity":null,"casperObjectId":466,"displayName":"","featType":"CF","featSubType":"Ranger","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>When Taking Aim at your Prey, gain an additional +3 to the attack roll per frame.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561058,"modifiedTime":1713405561058,"lastModifiedBy":null}}
{"name":"Silent Stalking","type":"Feat","img":"systems/newera-sol366/resources/ranger.png","_id":"CASPERFE00000467","system":{"description":"","rarity":null,"casperObjectId":467,"displayName":"","featType":"CF","featSubType":"Ranger","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":60,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":60,"description":"<p>You can move across Noisy Terrain at half your speed without suffering a Stealth penalty.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561059,"modifiedTime":1713405561059,"lastModifiedBy":null}}
{"name":"Unstoppable Force","type":"Feat","img":"systems/newera-sol366/resources/delver_arch.png","_id":"CASPERFE00000468","system":{"description":"","rarity":null,"casperObjectId":468,"displayName":"","featType":"AF","featSubType":"Path of Earth","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":160,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":160,"description":"<p>While Raging, you take half damage from all sources.</p>\n</p>(Damage is reduced by armor and resistances first, then divided by 2.)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561061,"modifiedTime":1713405561061,"lastModifiedBy":null}}
{"name":"Immovable Object","type":"Feat","img":"systems/newera-sol366/resources/delver_arch.png","_id":"CASPERFE00000469","system":{"description":"","rarity":null,"casperObjectId":469,"displayName":"","featType":"AF","featSubType":"Path of Earth","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>As a reaction, you can spend 20 energy to automatically succeed on a save against staggering, forced movement, or being knocked prone.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561062,"modifiedTime":1713405561062,"lastModifiedBy":null}}
{"name":"Fork Lightning","type":"Feat","img":"systems/newera-sol366/resources/delver_arch.png","_id":"CASPERFE00000470","system":{"description":"","rarity":null,"casperObjectId":470,"displayName":"","featType":"AF","featSubType":"Path of Wind","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":120,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":120,"description":"<p>You can target up to three creatures with any Evocation spell that inflicts Shock damage. Each target has its own chance to react, and you choose how the damage is divided amongst the targets you hit.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561063,"modifiedTime":1713405561063,"lastModifiedBy":null}}
{"name":"Wind at your Back","type":"Feat","img":"systems/newera-sol366/resources/delver_arch.png","_id":"CASPERFE00000471","system":{"description":"","rarity":null,"casperObjectId":471,"displayName":"","featType":"AF","featSubType":"Path of Wind","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>While Raging, you create a gust of wind that pushes you forward. Your speed increases by an additional 4 feet, and any creatures you move past during your turn are knocked Prone unless they succeed on an endurance save with difficulty equal to your current level.</p>\n</p>(Moving past a creature is defined as moving into and out of the creature's reach with a single or consecutive movement action(s).)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561064,"modifiedTime":1713405561064,"lastModifiedBy":null}}
{"name":"Deceleration","type":"Feat","img":"systems/newera-sol366/resources/delver_arch.png","_id":"CASPERFE00000472","system":{"description":"","rarity":null,"casperObjectId":472,"displayName":"","featType":"AF","featSubType":"Path of Wind","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":60,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":60,"description":"<p>You can use your channeling powers to create a cushion of air that protects you and your allies from falling damage.</p>\n</p>As a reaction, you can spend 10 energy to negate the damage from a long fall on yourself or a creature you can see.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561065,"modifiedTime":1713405561065,"lastModifiedBy":null}}
{"name":"Take to the Skies","type":"Feat","img":"systems/newera-sol366/resources/delver_arch.png","_id":"CASPERFE00000473","system":{"description":"","rarity":null,"casperObjectId":473,"displayName":"","featType":"AF","featSubType":"Path of Wind","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":200,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":200,"description":"<p>Your channeling powers can create a powerful updraft, lifting you off the ground and enabling sustained flight.</p>\n</p>You can hover or move up to 10 feet per frame while flying. Sustaining flight consumes 10 energy per frame.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561067,"modifiedTime":1713405561067,"lastModifiedBy":null}}
{"name":"Dualcaster","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000474","system":{"description":"","rarity":null,"casperObjectId":474,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>Most spells require only the use of one hand. By using both hands for simple spells, you've learned to make them more powerful.</p>\n</p>Whenever you cast a spell in your school of magic using with both hands free, it is automatically amplified to the next-highest factor. The casting difficulty remains unchanged, but the energy cost reflects the amplified level.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561068,"modifiedTime":1713405561068,"lastModifiedBy":null}}
{"name":"Magic Savant","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000475","system":{"description":"","rarity":null,"casperObjectId":475,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":400,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":400,"description":"<p>Gain the benefits from the other mode of Graduate Studies that you didn't choose.</p>\n</p>(If you chose Branch Out, then choose another school of magic to add your secondary proficiency bonus to. If you chose Double Major, then extend your secondary proficiency bonus to the other schools in the same form as your primary major.)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561069,"modifiedTime":1713405561069,"lastModifiedBy":null}}
{"name":"Spell Blending","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000476","system":{"description":"","rarity":null,"casperObjectId":476,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":3,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>You can cast two spells simultaneously.</p>\n</p>With both hands free, you can cast two quick spells in a single frame or two standard spells in two frames. Roll the spellcasting checks for each spell separately. The two spells may have the same or different targets.|(You must have both hands free to blend spells, even if you have the Asomatic Casting class feat.)</p><p>Tier 2: You can cast two sustained spells simultaneously. If your concentration is broken, both spells are interrupted.</p><p>Tier 3: You can cast two charged spells simultaneously. Each must be initiated at the same time. If your concentration is broken, both spells are interrupted.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561070,"modifiedTime":1713405561070,"lastModifiedBy":null}}
{"name":"Metamagical Experimentation","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000477","system":{"description":"","rarity":null,"casperObjectId":477,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>You've tinkered with the school of Metamagic, one of the most difficult arcane arts.</p>\n</p>Gain two levels in the Metamagic specialty. You may learn three common Metamagic enchantments from the Spell Study Guide.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561072,"modifiedTime":1713405561072,"lastModifiedBy":null}}
{"name":"Long Range Targeting","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000478","system":{"description":"","rarity":null,"casperObjectId":478,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":40,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":40,"description":"<p>You can cast Targeting spells on any valid target you can see. If the target is outside the spell's normal range, the casting difficulty increases by 10.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561074,"modifiedTime":1713405561074,"lastModifiedBy":null}}
{"name":"Asomatic Casting","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000481","system":{"description":"","rarity":null,"casperObjectId":481,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":60,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":60,"description":"<p>You can cast spells in your school of magic without the use of your hands, allowing you to cast spells while grappled, restrained, or incapacitated. You take a -5 penalty to the spellcasting check when doing so.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561076,"modifiedTime":1713405561076,"lastModifiedBy":null}}
{"name":"Triple Blending","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000482","system":{"description":"","rarity":null,"casperObjectId":482,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":150,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":150,"description":"<p>You can cast up to three spells simultaneously, without the use of your hands.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561077,"modifiedTime":1713405561077,"lastModifiedBy":null}}
{"name":"General Education","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000483","system":{"description":"","rarity":null,"casperObjectId":483,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":-1,"isUpgrade":true,"isSingleTiered":false,"totalCost":50,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":50,"description":"<p>Gain another instance of the Learning Experience core feature.</p>\n</p>(Gain one level in an existing Knowledge skill, or gain a new Knowledge with skill level 1.)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561078,"modifiedTime":1713405561078,"lastModifiedBy":null}}
{"name":"Magical Mimicry","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000484","system":{"description":"","rarity":null,"casperObjectId":484,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":110,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":110,"description":"<p>You can attempt to mimic spells you see in the wild.</p>\n</p>Once per day, whenever you see a creature cast a spell in your school of magic that you don't know, you can attept a Learn Spell check as a reaction with a -10 penalty. You don't need to be semi-conscious to use this ability.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561080,"modifiedTime":1713405561080,"lastModifiedBy":null}}
{"name":"Elementalist","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000485","system":{"description":"","rarity":null,"casperObjectId":485,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":70,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":70,"description":"<p>Gain access to the Delver archetype feats for the Elemental Path that corresponds to your school(s) of magic.</p>\n</p>(If an archetype feat from another class relies on a core feature from that class, you can't use it with this ability.)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561081,"modifiedTime":1713405561081,"lastModifiedBy":null}}
{"name":"Level-Headed","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000486","system":{"description":"","rarity":null,"casperObjectId":486,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":30,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":30,"description":"<p>You automatically succeed on Meditation checks to become Semi-Conscious.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561083,"modifiedTime":1713405561083,"lastModifiedBy":null}}
{"name":"Powerful Mind","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000487","system":{"description":"","rarity":null,"casperObjectId":487,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":40,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":40,"description":"<p>Your sharp mind is difficult to magically manipulate.</p>\n</p>You may spend 10 energy to give yourself a +5 bonus on a Determination save against any Psionic magical effect.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561084,"modifiedTime":1713405561084,"lastModifiedBy":null}}
{"name":"Long-Throw Spells","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000488","system":{"description":"","rarity":null,"casperObjectId":488,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>Double the range of Projectile spells you cast.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561086,"modifiedTime":1713405561086,"lastModifiedBy":null}}
{"name":"Prepared Casting","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000489","system":{"description":"","rarity":null,"casperObjectId":489,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":3,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>See: Prepared Spells</p>\n</p>You follow in the footsteps of the Scholar, learning to prepare certain spells for easier use.|You gain three Level 1 spell slots. As a Researcher, you can only prepare spells in your major(s).</p><p>Tier 2: You gain three Level 2 spell slots.</p><p>Tier 3: You gain three Level 3 spell slots.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561088,"modifiedTime":1713405561088,"lastModifiedBy":null}}
{"name":"Psychological Assault","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000492","system":{"description":"","rarity":null,"casperObjectId":492,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":60,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":60,"description":"<p>The difficulty of Determination saves called for by spells you cast increases by 5.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561091,"modifiedTime":1713405561091,"lastModifiedBy":null}}
{"name":"Magical Focus","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000493","system":{"description":"","rarity":null,"casperObjectId":493,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>See: Magical Focus</p>\n</p>You gain the ability to create a Focus, an object such as a wand or staff that stores spells for later use. You learn the Create Focus spell. Your focus can store up to 30 energy total.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561093,"modifiedTime":1713405561093,"lastModifiedBy":null}}
{"name":"Powerful Focus","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000494","system":{"description":"","rarity":null,"casperObjectId":494,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":-1,"isUpgrade":true,"isSingleTiered":false,"totalCost":20,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":20,"description":"<p>Increase the energy capacity of your focus by 10.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561094,"modifiedTime":1713405561094,"lastModifiedBy":null}}
{"name":"Tolgethic Scribe","type":"Feat","img":"systems/newera-sol366/resources/researcher.png","_id":"CASPERFE00000495","system":{"description":"","rarity":null,"casperObjectId":495,"displayName":"","featType":"CF","featSubType":"Researcher","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":180,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":180,"description":"<p>You've gained advanced knowledge of the Tolgethic lauguage - enough to create scripts of spells in your major(s), including those you have created.</p>\n</p>Using any writing implement and surface, you can create a spell script of any spell from one of your schools of magic by making a Spellcasting check with a base difficulty of 20. Add your Tolgethic knowledge modifier.|It takes two hours to create a script for a quick spell or cantrip, four hours for a standard or sustained spell, six hours ro a ritual, and eight hours for an enchantment.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561096,"modifiedTime":1713405561096,"lastModifiedBy":null}}
{"name":"Talk Up The Team","type":"Feat","img":"systems/newera-sol366/resources/chanter.png","_id":"CASPERFE00000496","system":{"description":"","rarity":null,"casperObjectId":496,"displayName":"","featType":"CF","featSubType":"Chanter","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":75,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":75,"description":"<p>You can make a positive impression of your allies. Whenever someone's Relationship to you improves, their relationship to all your allies improves by the same amount.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561098,"modifiedTime":1713405561098,"lastModifiedBy":null}}
{"name":"See Through Facades","type":"Feat","img":"systems/newera-sol366/resources/chanter.png","_id":"CASPERFE00000497","system":{"description":"","rarity":null,"casperObjectId":497,"displayName":"","featType":"CF","featSubType":"Chanter","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>You're incredibly good at telling when a person has ulterior motives. Add your Fount of Knowledge bonus to Sense Motive (Insight) checks.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561100,"modifiedTime":1713405561100,"lastModifiedBy":null}}
{"name":"Multitasker","type":"Feat","img":"systems/newera-sol366/resources/chanter.png","_id":"CASPERFE00000498","system":{"description":"","rarity":null,"casperObjectId":498,"displayName":"","featType":"CF","featSubType":"Chanter","currentTier":1,"maximumTier":2,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>You can cast standard and quick spells while chanting without breaking your concentration.</p><p>Tier 2: You can cast a sustained spell while chanting without breaking your concentration.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561103,"modifiedTime":1713405561103,"lastModifiedBy":null}}
{"name":"Spellcraft","type":"Feat","img":"systems/newera-sol366/resources/chanter.png","_id":"CASPERFE00000500","system":{"description":"","rarity":null,"casperObjectId":500,"displayName":"","featType":"CF","featSubType":"Chanter","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>You decide to try your hand at Spellcraft the art of creating new spells. As a Chanter, your Spellcraft skill is equal to your current Fount of Knowledge modifier.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561108,"modifiedTime":1713405561108,"lastModifiedBy":null}}
{"name":"Composer","type":"Feat","img":"systems/newera-sol366/resources/chanter.png","_id":"CASPERFE00000501","system":{"description":"","rarity":null,"casperObjectId":501,"displayName":"","featType":"CF","featSubType":"Chanter","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":240,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":240,"description":"<p>As an advanced Chanter, you've begun to try composing your own chants.</p>\n</p>You gain the ability to craft your own chants. Use the same process as creating a spell. The GM decides the level of the chant and any additional costs.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561112,"modifiedTime":1713405561112,"lastModifiedBy":null}}
{"name":"Art of the Blade","type":"Feat","img":"systems/newera-sol366/resources/investigator.png","_id":"CASPERFE00000502","system":{"description":"","rarity":null,"casperObjectId":502,"displayName":"","featType":"CF","featSubType":"Investigator","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":50,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":50,"description":"<p>You may use your Sleight of Hand modifier on attack rolls with daggers.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561115,"modifiedTime":1713405561115,"lastModifiedBy":null}}
{"name":"Overextending Feint","type":"Feat","img":"systems/newera-sol366/resources/investigator.png","_id":"CASPERFE00000503","system":{"description":"","rarity":null,"casperObjectId":503,"displayName":"","featType":"CF","featSubType":"Investigator","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":90,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":90,"description":"<p>Instead of feinting to confuse your opponent and attack them, you can force them to overextend on their attack.</p>\n</p>When you use the Feint (Deception) action for the first time each turn, you gain an opportunity attack on a regular success and two opprtunity frames on a critical success.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561117,"modifiedTime":1713405561117,"lastModifiedBy":null}}
{"name":"Plant Evidence","type":"Feat","img":"systems/newera-sol366/resources/investigator_arch.png","_id":"CASPERFE00000504","system":{"description":"","rarity":null,"casperObjectId":504,"displayName":"","featType":"AF","featSubType":"Marauder","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>Gain one level in the Pickpocketing (Sleight of Hand) specialty. Any time you could use Sleight of Hand to snatch or pickpocket an item, you can choose to instead instead place an item in the target's inventory.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561121,"modifiedTime":1713405561121,"lastModifiedBy":null}}
{"name":"Arcane Blade","type":"Feat","img":"systems/newera-sol366/resources/investigator.png","_id":"CASPERFE00000505","system":{"description":"","rarity":null,"casperObjectId":505,"displayName":"","featType":"CF","featSubType":"Investigator","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":50,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":50,"description":"<p>Sneak attacks with enchanted weapons increase the damage dealt by Augment enchantments rather than the weapon's normal damage.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561125,"modifiedTime":1713405561125,"lastModifiedBy":null}}
{"name":"Trap Inspector","type":"Feat","img":"systems/newera-sol366/resources/investigator.png","_id":"CASPERFE00000506","system":{"description":"","rarity":null,"casperObjectId":506,"displayName":"","featType":"CF","featSubType":"Investigator","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>Gain a +3 bonus on Perception and Logic checks to find and investigate traps.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561129,"modifiedTime":1713405561129,"lastModifiedBy":null}}
{"name":"Backstab","type":"Feat","img":"systems/newera-sol366/resources/investigator_arch.png","_id":"CASPERFE00000507","system":{"description":"","rarity":null,"casperObjectId":507,"displayName":"","featType":"AF","featSubType":"Assassin","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":120,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":120,"description":"<p>When performing a sneak attack with a knife or dagger from behind a target, the sneak attack damage is doubled.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561132,"modifiedTime":1713405561132,"lastModifiedBy":null}}
{"name":"Artifact Investigation","type":"Feat","img":"systems/newera-sol366/resources/investigator.png","_id":"CASPERFE00000508","system":{"description":"","rarity":null,"casperObjectId":508,"displayName":"","featType":"CF","featSubType":"Investigator","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":70,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":70,"description":"<p>When using any kind of magic detection, you know the level and school(s) of magic used, as well as the complexity.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561135,"modifiedTime":1713405561135,"lastModifiedBy":null}}
{"name":"Right Behind You","type":"Feat","img":"systems/newera-sol366/resources/investigator_arch.png","_id":"CASPERFE00000509","system":{"description":"","rarity":null,"casperObjectId":509,"displayName":"","featType":"AF","featSubType":"Assassin","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":100,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":100,"description":"<p>As a reaction, you can Tumble to move behind a target. This action takes a -5 penalty compared to a standard dodge, but grants you advantage on your next attack on a success.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561139,"modifiedTime":1713405561139,"lastModifiedBy":null}}
