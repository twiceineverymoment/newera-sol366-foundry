{"name":"Brains over Brawn","type":"Feat","img":"systems/newera-sol366/resources/scholar.png","_id":"CASPERFE00000364","system":{"description":"","rarity":null,"casperObjectId":364,"displayName":"","featType":"CF","featSubType":"Scholar","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":50,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":50,"description":"<p>You may use your Charisma modifier instead of Constitution to cast Divine spells.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405560904,"modifiedTime":1713405560904,"lastModifiedBy":null}}
{"name":"Lightning Reflexes","type":"Feat","img":"systems/newera-sol366/resources/scholar.png","_id":"CASPERFE00000365","system":{"description":"","rarity":null,"casperObjectId":365,"displayName":"","featType":"CF","featSubType":"Scholar","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":30,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":30,"description":"<p>When you enter combat, you may cast one of your prepared quick spells as a free reaction. (This takes place as a reaction immediately upon you entering combat, before the start of your first turn. This action can't be delayed, and can only occur once per combat encounter.)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405560906,"modifiedTime":1713405560906,"lastModifiedBy":null}}
