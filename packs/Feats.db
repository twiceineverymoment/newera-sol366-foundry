{"name":"Low Light Vision","type":"Feat","img":"systems/newera-sol366/resources/delver.png","_id":"CASPERFE00000357","system":{"description":"","rarity":null,"casperObjectId":357,"displayName":"","featType":"CF","featSubType":"Delver","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":80,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":80,"description":"<p>Delvers are used to hunting for shiny objects in dark caves. You gain low-light vision (you can see in dim light as though it were bright light.)</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405560897,"modifiedTime":1713405560897,"lastModifiedBy":null}}
