{"name":"Covert Entry Specialist","type":"Feat","img":"systems/newera-sol366/resources/delver.png","_id":"CASPERFE00000355","system":{"description":"","rarity":null,"casperObjectId":355,"displayName":"","featType":"CF","featSubType":"Delver","currentTier":1,"maximumTier":1,"isUpgrade":false,"isSingleTiered":false,"totalCost":25,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":25,"description":"<p>You have some experience opening locked doors and chests.</p>\n</p>Gain 1 point in the Lockpicking (Sleight of Hand) specialty.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405560895,"modifiedTime":1713405560895,"lastModifiedBy":null}}
