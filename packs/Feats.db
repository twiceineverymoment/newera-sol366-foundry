{"name":"Wilderness Knowledge","type":"Feat","img":"systems/newera-sol366/resources/ranger.png","_id":"CASPERFE00000459","system":{"description":"","rarity":null,"casperObjectId":459,"displayName":"","featType":"CF","featSubType":"Ranger","currentTier":1,"maximumTier":-1,"isUpgrade":true,"isSingleTiered":false,"totalCost":40,"bonuses":{},"choices":{},"prerequisites":{},"conflicts":{},"tiers":{"base":{"cost":40,"description":"<p>Gain one level in a nature- or animal-related Knowledge of your choice. The GM decides what qualifies for this feat.</p>"}}},"effects":[],"folder":null,"flags":{},"_stats":{"systemId":"newera-sol366","systemVersion":"0.12.6","coreVersion":"11.307","createdTime":1713405561050,"modifiedTime":1713405561050,"lastModifiedBy":null}}
